{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style":"off",
        "no-console": "off",
        "no-trailing-spaces":"off",
        "eol-last": "off",
        "no-multiple-empty-lines":"off",
        "indent": "off",
        "no-unused-vars":"off",
        "no-undef": "off",
        "no-use-before-define": "off",
        "consistent-return": "off"
    }
}